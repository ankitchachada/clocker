<% if current_user %>
	<%= form_tag(clock_events_path, method: "post") do %>
	 	<%= text_field_tag "clock_entry[event_type]" , "" ,{ placeholder: "Event Type"} %>
	  <%= submit_tag  clock_button %>
	<% end %>
<% end %>
<% @clock_events.each do |user| %>
  <%= user.user.email %> | <%= is_clocked(user) %><br/>
<% end %>
